/**
 * Definition for singly-linked list.
 * struct ListNode {
 *     int val;
 *     ListNode *next;
 *     ListNode(int x) : val(x), next(NULL) {}
 * };
 */
class Solution {
public:
    int kthToLast(ListNode* head, int k) {
        ListNode* node = head;
        int count = 0;
        while(node){
            count++;
            node = node->next;
        }
        node = head;
        for(int i=0; i<count-k; i++){
            node = node->next;
        }
        return node->val;
    }
};
